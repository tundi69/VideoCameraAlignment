<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cameraview.PreviewTestController">
   <children>
      <ImageView fx:id="previewTestImageView" fitHeight="634.0" layoutX="327.0" layoutY="68.0" preserveRatio="true" />
      <Button fx:id="startCamera1" layoutX="106.0" layoutY="448.0" mnemonicParsing="false" onAction="#startCamera" text="Start Camera" />
      <Button fx:id="stopCamera1" layoutX="106.0" layoutY="474.0" mnemonicParsing="false" onAction="#stopCamera" text="Stop Camera" />
      <Button fx:id="readAlignmentResult" layoutX="106.0" layoutY="68.0" mnemonicParsing="false" onAction="#readAlignmentResult" text="Try transition using alignment result" />
   </children>
</AnchorPane>
